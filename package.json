  "private": true,
  "scripts": {
    "build": "yarn clean && webpack-cli --config ./config/webpack.config.ts",
    "build:open": "yarn clean && webpack-cli --config ./config/webpack.config.ts && open-cli ./build/index.html",
    "clean": "del-cli build -f",
    "doctor": "yarn dlx @yarnpkg/doctor",
    "lint": "eslint './services/**/*.{ts, tsx}' './frontend/**/*.{ts, tsx}' --fix",
    "test": "jest --detectOpenHandles"
  },
  "workspaces": [
    "services/*",
    "frontend"
  ],
